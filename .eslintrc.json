{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": "standard-with-typescript",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "project": "tsconfig.json"
    },
    "rules": {
        "@typescript-eslint/strict-boolean-expressions": ["off"],
        "@typescript-eslint/no-floating-promises": ["off"],
        "no-prototype-builtins": ["off"]
    }
}
